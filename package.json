{
  "name": "csas-devops",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --ignore \"src/lib/client\"",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "biome check . && prettier \"./**/*.svelte\" --check",
    "format": "biome check --apply-unsafe . && biome format . --write && prettier \"./**/*.svelte\" --write",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\" && openapi --input openapi.json --output src/lib/client"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.3.3",
    "@greenpanda/svelte-layout": "^0.1.1",
    "@majkit/openapi": "^0.25.4",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.4",
    "fp-ts": "^2.16.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "sass": "^1.69.5",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2"
  },
  "type": "module"
}
